<odoo>

    <record id="chart_template" model="account.chart.template">
        <field name="name">Estonian Tax and Chart of Accounts Template (OCA)</field>
        <field name="currency_id" ref="base.EUR"/>
        <field name="code_digits">6</field>
        <field name="complete_tax_set" eval="True"/>
        <field name="use_anglo_saxon" eval="True"/>
        <field name="transfer_account_code_prefix">100</field>
        <field name="bank_account_code_prefix">101</field>
        <field name="cash_account_code_prefix">102</field>
    </record>

</odoo>
